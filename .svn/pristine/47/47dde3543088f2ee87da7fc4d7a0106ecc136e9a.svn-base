.stepwizard-step {
    display: table-cell;
    text-align: center;
    position: relative;
}
.btn-circle {
  width: 25px;
  height: 25px;
  display: inline-flex;
  align-items: center;
  justify-content: center;
  font-size: 12px;
  /*line-height: 1.428571429;*/
  border-radius: 50%;
  padding: 0;
}
.btn-danger.step-done{
    color: #fff;
    background-color: #fff;
    border-color: #de4941;
}
.btn-danger:not(:disabled):not(.disabled).active:focus, .btn-danger:not(:disabled):not(.disabled):active:focus, .show>.btn-danger.dropdown-toggle:focus {
    box-shadow: 0 0 0 0rem rgba(220,53,69,.5);
}
.btn-danger:not(:disabled):not(.disabled).active, .btn-danger:not(:disabled):not(.disabled):active {
    color: #fff;
    background-color: #de4941;
    border-color: #de4941;
}
.btn-danger[disabled], .btn-danger.disabled{
    color: #c6c6c6;
    background-color: #fff;
    border: 2px solid #ddd;
}
p.disabled{
    color: #ddd;
}
.btn-danger {
    color: #444;
    background-color: #fff;
    border: 2px solid #de4941;
}
.btn-danger:focus,.btn-danger:hover {
    color: #fff;
    background-color: #de4941;
    border-color: #de4941;
}
.sku-assign{
    font-size: 16px;
}
.sku-assign span{
    color: #de4941 !important;
}
.stepwizard-step a{
    cursor: not-allowed !important;
    pointer-events: none;
}
.btn-circle.step-complete:after, .stepwizard-step:first-child .btn-circle.active:after {
    content: '';
    height: 2px;
    background-color: #de4941;
    position: absolute;
    right: 0;
    left: 50%;
    top: 25%;
    z-index: -1;
}
.stepwizard-step:first-child .btn-circle:before, .stepwizard-step:last-child .btn-circle:after{
    display: none;
}
.btn-circle.step-complete:before, .stepwizard-step:last-child .btn-circle.active:before, .stepwizard-step .btn-circle.active:before {
    content: '';
    height: 2px;
    background-color: #de4941;
    position: absolute;
    right: 50%;
    left: 0;
    top: 25%;
    z-index: -1;
}
.btn-circle:before{
    content: '';
    height: 2px;
    background-color: #ddd;
    position: absolute;
    right: 50%;
    left: 0;
    top: 25%;
    z-index: -1;
}
.btn-circle:after, .stepwizard-step:first-child .btn-circle.active:after{
    content: '';
    height: 2px;
    background-color: #ddd;
    position: absolute;
    right: 0;
    left: 50%;
    top: 25%;
    z-index: -1;
}
.stepper_btn {
   position: sticky;
   top: -25px;
   background-color: #fafafa;
   z-index: 1;
   padding-top: 0.5rem;
}