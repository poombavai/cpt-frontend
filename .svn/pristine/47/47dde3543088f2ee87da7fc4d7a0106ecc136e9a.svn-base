/*Checkbox*/

input[type="checkbox"]{
  width: 20px;
  height: 20px;
  margin-top: 0px;
}

input[type="checkbox"]:disabled:before {
    content: ' ';
    font-size: 12px;
    font-weight: 900;
    opacity: 1;
    width: 20px;
    height: 20px;
    border-radius: 2px;
    border: 1px solid #cdcdcd;
    background-color: #fff;
    position: absolute;
}

input[type="checkbox"]:checked:disabled:after {
    content: '✔';
    color: #de4941;
    font-size: 12px;
    font-weight: 900;
    opacity: 1;
    width: 20px;
    height: 20px;
    border-radius: 2px;
    border: 1px solid #cdcdcd;
    background-color: #fff;
    position: absolute;
}

