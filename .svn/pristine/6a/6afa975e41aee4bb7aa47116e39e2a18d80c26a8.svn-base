        <script type="text/javascript">
            function addmore(add) {
                var p = add.parentElement;
                var sp = p.parentElement;
                var mp = sp.parentElement;
                var hp = mp.parentElement;
                var lastdiv = hp.lastElementChild;
                var lastchild = lastdiv;
                var text = sp.firstElementChild.value;
                if (text != "") {
                    lastchild.innerHTML += '<div class="col-md-6 col-sm-12 mt-3"><div class="d-flex align-items-center"><input type="checkbox" name="" class="mr-2"><span>' + text + '</span></div></div>';
                }
            }
